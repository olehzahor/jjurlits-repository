<?xml version="1.0" encoding="UTF-8"?>
<addons>
    <addon id="repository.jjurlits" name="jjurlits repo" version="0.0.1" provider-name="Jurij Jurlits">
        <requires>
            <import addon="xbmc.addon" version="12.0.0"/>
        </requires>
        <extension point="xbmc.addon.repository" name="jjurlits repo">
            <info compressed="false">https://raw.githubusercontent.com/olehzahor/jjurlits-repository/master/_repo/addons.xml</info>
            <checksum>https://raw.githubusercontent.com/olehzahor/jjurlits-repository/master/_repo/addons.xml.md5</checksum>
            <datadir zip="true">https://raw.githubusercontent.com/olehzahor/jjurlits-repository/master/_repo/</datadir>
            <hashes>false</hashes>
        </extension>
        <extension point="xbmc.addon.metadata">
            <summary></summary>
            <description></description>
            <platform>all</platform>
    </extension>
</addon>

<addon id="service.script.integrator"
    name="Integrator"
    version="0.0.2"
    provider-name="">
    <requires>
        <import addon="xbmc.python" version="2.1.0"/>
        <import addon="script.module.pydevd" version="3.4.1"/>
    </requires>
    <extension point="xbmc.service" library="service.py" start="login" />
    <extension point="xbmc.addon.metadata">
        <platform>all</platform>
        <summary lang="en"></summary>
    </extension>
</addon>
</addons>
